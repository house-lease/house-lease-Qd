/* 清除内外边距 */
view,
input,
form,
image {
  padding: 0px;
  margin: 0px;
}

/* 设置轮播图 */
swiper {
  width: 100%;
  height: 400rpx;
}

image {
  height: 400rpx;
  width: 100%;
}

.swiperContainer {
  position: relative;
}

/*图片数量*/
.imageCount {
  width: 100rpx;
  height: 50rpx;
  background-color: rgba(12, 2, 2, 0.741);
  border-radius: 40rpx;
  line-height: 50rpx;
  color: #fff;
  text-align: center;
  font-size: 26rpx;
  position: absolute;
  left: 240px;
  bottom: 20rpx;
}

.price {
  padding-right: 5rpx;
  font-size: 20px;
  color: red;
}

text {
  font-weight: 500;
}

.houseType {
  padding-right: -5rpx;
}

.wo,
.jia {
  margin-left: 420rpx;
  margin-top: -110rpx;
  margin-bottom: 40rpx;
  font-size: 18px;
  font-weight: bold;
}

.particulars {
  margin: 20rpx;
  border-radius: 50%;
}

.phoneImage {
  margin-left: 180rpx;
  width: 50rpx;
  height: 50rpx;

}

/*分割线样式*/
.divLine {
  margin-top: 40rpx;
  background: rgba(128, 125, 125, 0.59);
  width: 100%;
  height: 4rpx;
}

/* 详情信息*/
.message {
  font-size: 16px;
  margin-top: 50rpx;
  margin-left: 10rpx;
}

.message,
text {
  padding-left: 30rpx;
}

.messageList {
  margin: 30rpx;
  margin-left: 60rpx;
  font-size: 15px;
  color: rgba(162, 153, 153, 0.714);
}

.messageLis,
text {
  padding-left: 80rpx;
}

.jianjie {
  font-weight: 600;
}

.intro image {
  width: 100rpx;
  height: 100rpx;
}

/* 房屋简介*/
.jianjie {
  display: block;
  text-align: center;
  padding-left: 0;
}

.furniture-stype,
.jianjietext {
  display: flex;
  justify-content: space-evenly;
}

.jianjietext text {
  padding: 20rpx;
  display: inline-block;
  text-align: center;
  width: 120rpx;
}

/* 房屋位置*/
.location {
  margin: 20rpx;
}

/* 租客评价*/
.estimate {
  margin: 0;
  padding: 0;
}

.zkpj {
  font-weight: 800;
  padding-left: 0;
  display: inline;
  text-align: center;
  font-size: 50rpx;
}

.pingjia {
  width: 100rpx;
  height: 80rpx;
  margin-top: 50rpx;
  margin-bottom: 0;
}

.pingjia .grade {
  margin-left: 160rpx;
  margin-top: -100rpx;
}

.pingjia,
.pjtx {
  width: 100rpx;
  height: 100rpx;
  font-weight: 900;
  font-size: 50rpx;
}

.pingjia image {
  width: 150rpx;
  height: 150rpx;
}

.pingjias {
  margin-top: -20rpx;
  margin-left: 400rpx;
  font-size: 36rpx;
  font-weight: 900;
}

.userpingjia {}


.estimateContext {
  margin-top: 20rpx;
  margin-left: 20rpx;
  width: 120rpx;
  height: 120rpx;
}

.tx {
  width: 120rpx;
  height: 120rpx;
  padding: 0rpx;
}

.ping-jia {
  margin-top: -50rpx;
}

.tenantName {
  display: inline-block;
  font-size: 14px;
  padding: 0rpx;
  margin-left: 160rpx;
}

.contexts {
  font-size: 12px;
  margin: 30rpx;
  margin-left: 80rpx;
  height: 100rpx;
  font-size: 36rpx;
  line-height: initial;
  font-weight: 500;
}

.bottom {
  height: 140rpx;
  position: relative;
  left: 0;
  right: 0;
  top: 0;
  display: flex;
  text-align: center;
  justify-content: space-evenly;
  background-color: D7d7d7;
  font-size: 30rpx;
}

.bottom image {
  width: 70rpx;
  height: 70rpx;
}

.bottom button {
  background-color: bule;
}

.bottom[disabled],
wx-button {
  color: #7f8389;
  background-color: blue;
  border-color: blue;
  width: 100rpx;
}

