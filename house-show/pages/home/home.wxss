/* 房屋列表 */
.lists{
  padding:30rpx 30rpx 0rpx 60rpx;
}
.lists::after{
  content:" ";
  display:block;
  clear:both;
}
.tu,.message{
  float: left;
}
.tu image{
  width: 300rpx;
  height: 180rpx;
}
.message{
  width: 46%;
  margin-left: 50rpx;
  border-bottom: 1px solid rgba(192,192,192);
}
