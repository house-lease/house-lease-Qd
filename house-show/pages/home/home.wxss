

/* 清除内外边距 */
view,input,form,image{
  padding: 0px;
  margin: 0px;
}


/* 位置样式 */
.pa::after{
  content:" ";
  display:block;
  clear:both;
}
.pa{
  margin-bottom: 10rpx;
}
.weiZhi{
  display: inline-block;
  position: relative;
  font-size: 24rpx;
  padding-right: 40rpx;
}


/* 下拉图标 */
.xiaLa,.souSuoImg{
  width: 20rpx;
  height: 20rpx;
}
.xiaLa{
  position: absolute;
  top: 5rpx;
  left: 52rpx;
}
.weizhi_{
  float: left;
  margin-left: 40rpx;
  border: rgba(192,192,192) solid 1px;
  border-right: none;
  /* margin-left:40rpx; */
  padding: 20rpx;
  box-shadow: 0rpx 5rpx 5rpx 0rpx rgba(192,192,192);
}

/* 横着的分割View样式 */
/*分割线样式*/
.divLine{
