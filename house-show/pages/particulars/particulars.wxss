/* 清除内外边距 */
view,
input,
form,
image {
  font-family: "楷体";
  padding: 0px;
  margin: 0px;
}

/* 设置轮播图 */
swiper {
  width: 100%;
  height: 400rpx;
}

image {
  height: 400rpx;
  width: 100%;
}

.swiperContainer {
  position: relative;
  margin-top: 20rpx;
}

/*图片数量*/
.imageCount {
  width: 100rpx;
  height: 50rpx;
  background-color: rgba(12, 2, 2, 0.741);
  border-radius: 40rpx;
  line-height: 50rpx;
  color: #fff;
  text-align: center;
  font-size: 26rpx;
  position: absolute;
  left: 300px;
  bottom: 20rpx;
}
.im{
  float: right;
  margin-right: 30rpx;
}
.price {

  font-weight: bold;
  font-size: 16px;
  color:tomato;
}

.xin{
  font-size: 16px;
  margin-top: 20rpx;
  margin-bottom: 30rpx;
  margin-left: 40rpx;
  font-family: Microsoft Yahei;
}
.money{
  font-family: Microsoft Yahei;
  display: inline-block;
  margin-left: 40rpx;
  margin-top: 20rpx;
}

text {
  font-weight: 500;
}
.jianjie{
  margin-left: 40rpx;
  margin-top: 60rpx;
  font-family: Microsoft Yahei;
  font-size: 18px;
}
.qi{
  font-family: Microsoft Yahei;
  font-size: 14px;
  margin-left: 20rpx;
}
.houseType {
  margin-left: 100rpx;
}
.wo{
  margin-left: 40rpx;
}
.jia {
  font-size: 20px;
  font-weight: bold;
  font-family: Microsoft Yahei;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.particulars {
  margin: 20rpx;
  border-radius: 50%;
}

.phoneImage {
  margin-left: 180rpx;
  margin-top: 10rpx;
  width: 60rpx;
  height: 60rpx;
}

/*分割线样式*/

/* 详情信息*/

.intro image {
  width: 70rpx;
  height: 70rpx;
}
.intro::after{
  content:" ";
  display:block;
  clear:both;
}
/* 房屋简介*/
.zhengZu{
  float: left;
  margin-left: 90rpx;
  margin-top: 40rpx;
}
.t,.t1{
  margin-left: 10rpx;
  font-size: 20rpx;
  color: rgba(128,128,128);
}
.t1{
  font-weight: 400;
  text-decoration:line-through;
}

/* 房屋位置*/
.location {
  margin-top: 30rpx;
  width: 100%;
  height: 600rpx;
  margin-bottom: 200rpx;
}
.map_container{

  position: relative;
  width: 100%;
  height: 100%;
}
.map_container_top{
  height: 100%;
}
.map{
  width: 100%;
  height: 100%;
}

.bottom {

  width: 100%;
  height: 130rpx;
  position: fixed;
 bottom: 0rpx;
  background-color:white;
  font-size: 30rpx;
  display: flex;
  justify-content: space-around;
}

.shouCang image, .fenxiang image{
  width: 60rpx;
  height: 60rpx;
}
.shouCang, .fenxiang{
  padding-top: 30rpx;
  width: 80rpx;
  height: 80rpx;
}
.bottom text{
  padding-left: 0;
}

button[size='mini'] {
  margin-top: 40rpx;
  width: 100%;
  padding: 0 0.75em;
  line-height: 4;
  background-color: #169AD4;
  color: #fff;
  font-weight: 300;
  }
  .zi{
    font-size: 20rpx;
    display: block;
  }
  


  .section{
    height: 60rpx;
    width: 100%;
  }
  .section input{
    width:90%;
    margin:10rpx auto;
    border:1px solid #c3c3c3;
    height:60rpx;
    border-radius: 6rpx;
    padding: 0rpx 10rpx;
  }

  .map_text{
    position: absolute;
    left: 0rpx;
    right: 0rpx;
   bottom: 0rpx;
    height: 160rpx;
    background: #fff;
    padding: 0rpx 30rpx;
  }
  .h2{
    margin: 10rpx 0rpx;
    display: block;
    font-size:24rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
  }
  .h1{
    margin: 30rpx 0rpx;
    font-size:30rpx;
  }
  .weizhi_,.zhou{
    display: inline-block;
    margin-left: 30rpx;

    font-family: Microsoft Yahei;
    font-size: 16px;
  }
  .divLine_{
 
    border-top: 1rpx solid gray;
    padding-top: 30rpx;
    padding-bottom: 30rpx;
  }
  .xuan view{
    display: inline-block;
    margin-left: 80rpx;
    margin-bottom: 20rpx;
    margin-top: 20rpx;
  }