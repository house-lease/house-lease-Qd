<!-- 自定义组件 -->
<view>
<searcher></searcher>
</view>
<view class="container">
  <view class="Split"></view>
</view>
<!-- 条件筛选框 -->
<view class="tiaoJian">
<view class="t1">时间<image class="i1" src="/pages/image/xl.png"></image> </view>
<view class="t2">类型<image class="i2" src="/pages/image/xl.png"></image></view>
<view class="t3">价格<image class="i3" src="/pages/image/xl.png"></image></view>
</view>
<view>
  <view class="split1"></view>
</view>
<view>
  <view wx:for="{{houses}}" wx:for-index="key" wx:for-item="value" wx:key="id">
    <view class="flex-wrp" style="flex-direction:column;">
      <view class="tupian"><image class="i4" src="{{images[0]}}"></image></view>
         
      <view class="v1">
        <view class="typeAndNum">{{houses[key].type}}|可住{{houses[key].num}}人</view>
        <view class="address">{{houses[key].address}}</view>
      </view>
      <view class="v2">
        <view class="name">{{houses[key].name}}</view>
        <view class="img"><image class="phone" src="/pages/image/ph.png"></image></view>
      </view>     
      <view class="v3">
        <view class="money">￥{{houses[key].money}}</view>
        <view class="consult">电话咨询</view>
      </view> 
  </view>
  </view>
  
</view>
